{
    "name": "bizmate/php-test-dto",
    "type": "library",
    "description": "Defines test data objects and interface(s).",
    "license": "proprietary",
    "authors": [
        {
            "name": "Diego Gullo",
            "email": "diego_gullo@bizmate.biz"
        }
    ],
    "require": {
        "php": ">=7.0.22",
        "doctrine/common": "2.7.x-dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mockery/mockery": "^1.3.1",
        "pdepend/pdepend": "^2.8",
        "phpmd/phpmd": "^2.8",
        "phpmetrics/phpmetrics": "^2.7",
        "phpunit/phpunit": "^6.5",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "thibautselingue/local-php-security-checker-installer": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bizmate\\PhpTestDto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bizmate\\PhpTestDto\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "local-php-security-checker-installer && local-php-security-checker": "script"
        }
    }
}
